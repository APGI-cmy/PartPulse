{
  "page": {
    "size": "A4",
    "margin": 25,
    "orientation": "portrait"
  },
  "header": {
    "logos": [
      {
        "src": "/assets/logo/trane-logo.svg",
        "x": 25,
        "y": 25,
        "width": 100,
        "height": 40
      },
      {
        "src": "/assets/logo/trane-tech-logo.svg",
        "x": 470,
        "y": 25,
        "width": 100,
        "height": 40
      }
    ],
    "title": {
      "text": "TRANE WARRANTY PARTS CLAIMS FORM",
      "x": 0,
      "y": 95,
      "align": "center",
      "fontSize": 16,
      "bold": true
    }
  },
  "sections": [
    {
      "type": "spacer",
      "spacer": {
        "height": 25
      }
    },
    {
      "type": "line",
      "line": {
        "x1": 25,
        "x2": 570,
        "strokeWidth": 1.5
      }
    },
    {
      "type": "spacer",
      "spacer": {
        "height": 18
      }
    },
    {
      "type": "text",
      "label": "Section Header",
      "text": {
        "content": "GENERAL INFORMATION",
        "x": 25,
        "fontSize": 12,
        "align": "left"
      }
    },
    {
      "type": "spacer",
      "spacer": {
        "height": 12
      }
    },
    {
      "type": "fields",
      "fields": [
        {
          "label": "DATE:",
          "binding": "date",
          "x": 25,
          "labelWidth": 120,
          "width": 220,
          "fontSize": 10,
          "bold": true,
          "format": "date"
        },
        {
          "label": "CHILLER MODEL:",
          "binding": "chillerModel",
          "x": 25,
          "labelWidth": 180,
          "width": 270,
          "fontSize": 10,
          "bold": true
        },
        {
          "label": "CHILLER SERIAL NUMBER:",
          "binding": "chillerSerial",
          "x": 25,
          "labelWidth": 200,
          "width": 270,
          "fontSize": 10,
          "bold": true
        },
        {
          "label": "JOB NUMBER / SSID #:",
          "binding": "ssidJobNumber",
          "x": 25,
          "labelWidth": 180,
          "width": 270,
          "fontSize": 10,
          "bold": true
        },
        {
          "label": "BUILDING NAME:",
          "binding": "buildingName",
          "x": 25,
          "labelWidth": 150,
          "width": 270,
          "fontSize": 10,
          "bold": true
        },
        {
          "label": "SITE NAME:",
          "binding": "siteName",
          "x": 25,
          "labelWidth": 120,
          "width": 270,
          "fontSize": 10,
          "bold": true
        },
        {
          "label": "ATTENDED BY:",
          "binding": "technicianName",
          "x": 25,
          "labelWidth": 140,
          "width": 270,
          "fontSize": 10,
          "bold": true
        }
      ]
    },
    {
      "type": "spacer",
      "spacer": {
        "height": 25
      }
    },
    {
      "type": "line",
      "line": {
        "x1": 25,
        "x2": 570,
        "strokeWidth": 1.5
      }
    },
    {
      "type": "spacer",
      "spacer": {
        "height": 18
      }
    },
    {
      "type": "text",
      "label": "Parts Section",
      "text": {
        "content": "PARTS DETAILS",
        "x": 25,
        "fontSize": 12,
        "align": "left"
      }
    },
    {
      "type": "spacer",
      "spacer": {
        "height": 12
      }
    },
    {
      "type": "table",
      "table": {
        "columns": [
          {
            "header": "Part No.",
            "binding": "partNo",
            "width": 85
          },
          {
            "header": "Qty",
            "binding": "quantity",
            "width": 50,
            "format": "number"
          },
          {
            "header": "Failed Serial",
            "binding": "failedPartSerial",
            "width": 115
          },
          {
            "header": "Replaced Serial",
            "binding": "replacedPartSerial",
            "width": 115
          },
          {
            "header": "Failure Date",
            "binding": "dateOfFailure",
            "width": 85,
            "format": "date"
          },
          {
            "header": "Repair Date",
            "binding": "dateOfRepair",
            "width": 85,
            "format": "date"
          }
        ],
        "rowsBinding": "items",
        "x": 25,
        "headerHeight": 32,
        "rowHeight": 28,
        "fontSize": 9
      }
    },
    {
      "type": "spacer",
      "spacer": {
        "height": 25
      }
    },
    {
      "type": "line",
      "line": {
        "x1": 25,
        "x2": 570,
        "strokeWidth": 1.5
      }
    },
    {
      "type": "spacer",
      "spacer": {
        "height": 18
      }
    },
    {
      "type": "text",
      "label": "Comments Header",
      "text": {
        "content": "COMMENTS",
        "x": 25,
        "fontSize": 12,
        "align": "left"
      }
    },
    {
      "type": "spacer",
      "spacer": {
        "height": 8
      }
    },
    {
      "type": "text",
      "text": {
        "binding": "comments",
        "x": 25,
        "width": 545,
        "height": 70,
        "fontSize": 10,
        "multiline": true
      }
    },
    {
      "type": "spacer",
      "spacer": {
        "height": 20
      }
    },
    {
      "type": "checkbox",
      "checkbox": {
        "label": "TICK IF COVERED BY WARRANTY",
        "binding": "coveredByWarranty",
        "x": 25,
        "size": 14
      }
    },
    {
      "type": "spacer",
      "spacer": {
        "height": 25
      }
    },
    {
      "type": "line",
      "line": {
        "x1": 25,
        "x2": 570,
        "strokeWidth": 1.5
      }
    },
    {
      "type": "spacer",
      "spacer": {
        "height": 18
      }
    },
    {
      "type": "text",
      "label": "Notes Header",
      "text": {
        "content": "IMPORTANT NOTES",
        "x": 25,
        "fontSize": 12,
        "align": "left"
      }
    },
    {
      "type": "spacer",
      "spacer": {
        "height": 8
      }
    },
    {
      "type": "text",
      "text": {
        "content": "* Need Serial Number to be able to make a claim (Mandatory).",
        "x": 25,
        "fontSize": 9
      }
    },
    {
      "type": "text",
      "text": {
        "content": "* Provide Photos of Failed Parts.",
        "x": 25,
        "fontSize": 9
      }
    },
    {
      "type": "text",
      "text": {
        "content": "* Provide Service Reports for Failed Parts.",
        "x": 25,
        "fontSize": 9
      }
    },
    {
      "type": "spacer",
      "spacer": {
        "height": 30
      }
    },
    {
      "type": "line",
      "line": {
        "x1": 25,
        "x2": 570,
        "strokeWidth": 1.5
      }
    },
    {
      "type": "spacer",
      "spacer": {
        "height": 18
      }
    },
    {
      "type": "text",
      "label": "Admin Section",
      "text": {
        "content": "OFFICE ADMIN USE ONLY",
        "x": 25,
        "fontSize": 12,
        "align": "left"
      }
    },
    {
      "type": "spacer",
      "spacer": {
        "height": 12
      }
    },
    {
      "type": "stamp",
      "stamp": {
        "text": "PROCESSED",
        "binding": "adminProcessedStamp",
        "x": 450,
        "fontSize": 18,
        "rotation": -15,
        "color": "#FF2B00"
      }
    },
    {
      "type": "fields",
      "fields": [
        {
          "label": "Admin Date:",
          "binding": "adminDate",
          "x": 25,
          "labelWidth": 120,
          "width": 170,
          "fontSize": 10,
          "format": "date"
        }
      ]
    },
    {
      "type": "spacer",
      "spacer": {
        "height": 12
      }
    },
    {
      "type": "signature",
      "signature": {
        "label": "Admin Signature:",
        "binding": "adminSignature",
        "x": 25,
        "width": 280
      }
    },
    {
      "type": "spacer",
      "spacer": {
        "height": 18
      }
    },
    {
      "type": "signature",
      "signature": {
        "label": "Technician Signature:",
        "binding": "technicianSignature",
        "x": 25,
        "width": 280
      }
    }
  ]
}
