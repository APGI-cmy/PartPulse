{
  "page": {
    "size": "A4",
    "margin": 25,
    "orientation": "portrait"
  },
  "header": {
    "logos": [
      {
        "src": "/assets/logo/trane-logo.svg",
        "x": 25,
        "y": 25,
        "width": 100,
        "height": 40
      },
      {
        "src": "/assets/logo/trane-tech-logo.svg",
        "x": 470,
        "y": 25,
        "width": 100,
        "height": 40
      }
    ],
    "title": {
      "text": "TRANE INTERNAL PARTS",
      "x": 0,
      "y": 95,
      "align": "center",
      "fontSize": 16,
      "bold": true,
      "underline": false
    },
    "subtitle": {
      "text": "TRANSFER ORDER FORMS",
      "x": 0,
      "y": 112,
      "align": "center",
      "fontSize": 14,
      "bold": false
    }
  },
  "sections": [
    {
      "type": "spacer",
      "spacer": {
        "height": 25
      }
    },
    {
      "type": "line",
      "line": {
        "x1": 25,
        "x2": 570,
        "strokeWidth": 1.5
      }
    },
    {
      "type": "spacer",
      "spacer": {
        "height": 18
      }
    },
    {
      "type": "text",
      "label": "Section Header",
      "text": {
        "content": "GENERAL INFORMATION",
        "x": 25,
        "fontSize": 12,
        "align": "left"
      }
    },
    {
      "type": "spacer",
      "spacer": {
        "height": 12
      }
    },
    {
      "type": "fields",
      "fields": [
        {
          "label": "Date:",
          "binding": "createdAt",
          "x": 25,
          "labelWidth": 120,
          "width": 220,
          "fontSize": 10,
          "bold": true,
          "format": "date"
        },
        {
          "label": "SSID/PSID Number:",
          "binding": "serial",
          "x": 25,
          "labelWidth": 150,
          "width": 270,
          "fontSize": 10,
          "bold": true
        },
        {
          "label": "Site Name:",
          "binding": "department",
          "x": 25,
          "labelWidth": 120,
          "width": 270,
          "fontSize": 10,
          "bold": true
        },
        {
          "label": "PO Number:",
          "binding": "part",
          "x": 25,
          "labelWidth": 120,
          "width": 270,
          "fontSize": 10,
          "bold": true
        },
        {
          "label": "Technician Name:",
          "binding": "technician",
          "x": 25,
          "labelWidth": 150,
          "width": 270,
          "fontSize": 10,
          "bold": true
        }
      ]
    },
    {
      "type": "spacer",
      "spacer": {
        "height": 15
      }
    },
    {
      "type": "signature",
      "signature": {
        "label": "Technician Signature:",
        "binding": "signature",
        "x": 25,
        "width": 280
      }
    },
    {
      "type": "spacer",
      "spacer": {
        "height": 25
      }
    },
    {
      "type": "line",
      "line": {
        "x1": 25,
        "x2": 570,
        "strokeWidth": 1.5
      }
    },
    {
      "type": "spacer",
      "spacer": {
        "height": 18
      }
    },
    {
      "type": "text",
      "label": "Parts Section",
      "text": {
        "content": "PARTS DETAILS",
        "x": 25,
        "fontSize": 12,
        "align": "left"
      }
    },
    {
      "type": "spacer",
      "spacer": {
        "height": 12
      }
    },
    {
      "type": "table",
      "table": {
        "columns": [
          {
            "header": "QTY",
            "binding": "quantity",
            "width": 70,
            "format": "number"
          },
          {
            "header": "PART NO.",
            "binding": "partNo",
            "width": 160
          },
          {
            "header": "DESCRIPTION",
            "binding": "description",
            "width": 295
          }
        ],
        "rowsBinding": "items",
        "x": 25,
        "headerHeight": 28,
        "rowHeight": 28,
        "fontSize": 9
      }
    },
    {
      "type": "spacer",
      "spacer": {
        "height": 25
      }
    },
    {
      "type": "line",
      "line": {
        "x1": 25,
        "x2": 570,
        "strokeWidth": 1.5
      }
    },
    {
      "type": "spacer",
      "spacer": {
        "height": 18
      }
    },
    {
      "type": "text",
      "label": "Client Section Header",
      "text": {
        "content": "CLIENT COLLECTION",
        "x": 25,
        "fontSize": 12,
        "align": "left"
      }
    },
    {
      "type": "spacer",
      "spacer": {
        "height": 8
      }
    },
    {
      "type": "text",
      "text": {
        "content": "Below is for parts that are sold and collected by client...",
        "x": 25,
        "fontSize": 9
      }
    },
    {
      "type": "spacer",
      "spacer": {
        "height": 12
      }
    },
    {
      "type": "fields",
      "fields": [
        {
          "label": "Client Name:",
          "binding": "clientName",
          "x": 25,
          "labelWidth": 120,
          "width": 270,
          "fontSize": 10
        }
      ]
    },
    {
      "type": "spacer",
      "spacer": {
        "height": 12
      }
    },
    {
      "type": "fields",
      "fields": [
        {
          "label": "Date:",
          "binding": "clientDate",
          "x": 25,
          "labelWidth": 120,
          "width": 170,
          "fontSize": 10,
          "format": "date"
        }
      ]
    },
    {
      "type": "spacer",
      "spacer": {
        "height": 12
      }
    },
    {
      "type": "signature",
      "signature": {
        "label": "Client Signature:",
        "binding": "clientSignature",
        "x": 25,
        "width": 280
      }
    },
    {
      "type": "spacer",
      "spacer": {
        "height": 25
      }
    },
    {
      "type": "line",
      "line": {
        "x1": 25,
        "x2": 570,
        "strokeWidth": 1.5
      }
    },
    {
      "type": "spacer",
      "spacer": {
        "height": 18
      }
    },
    {
      "type": "text",
      "label": "Admin Section",
      "text": {
        "content": "OFFICE ADMIN USE ONLY",
        "x": 25,
        "fontSize": 12,
        "align": "left"
      }
    },
    {
      "type": "spacer",
      "spacer": {
        "height": 12
      }
    },
    {
      "type": "stamp",
      "stamp": {
        "text": "PROCESSED",
        "binding": "adminProcessedStamp",
        "x": 450,
        "fontSize": 18,
        "rotation": -15,
        "color": "#FF2B00"
      }
    },
    {
      "type": "checkbox",
      "checkbox": {
        "label": "A",
        "binding": "adminOptionA",
        "x": 25,
        "size": 14
      }
    },
    {
      "type": "spacer",
      "spacer": {
        "height": 8
      }
    },
    {
      "type": "checkbox",
      "checkbox": {
        "label": "B",
        "binding": "adminOptionB",
        "x": 25,
        "size": 14
      }
    },
    {
      "type": "spacer",
      "spacer": {
        "height": 12
      }
    },
    {
      "type": "fields",
      "fields": [
        {
          "label": "Date:",
          "binding": "adminDate",
          "x": 25,
          "labelWidth": 120,
          "width": 170,
          "fontSize": 10,
          "format": "date"
        }
      ]
    },
    {
      "type": "spacer",
      "spacer": {
        "height": 12
      }
    },
    {
      "type": "signature",
      "signature": {
        "label": "Admin Signature:",
        "binding": "adminSignature",
        "x": 25,
        "width": 280
      }
    }
  ]
}
